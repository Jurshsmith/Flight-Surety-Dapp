{
  "name": "flightsurety",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/techbubble/flightsurety",
  "license": "MIT",
  "scripts": {
    "truffle": "truffle",
    "webpack": "webpack",
    "gulp": "gulp",
    "webpack-dev-server": "webpack-dev-server",
    "test:contracts:app": "concurrently --success first --kill-others \"ganache-cli -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat'\" \"truffle test ./tests/FlightSuretyApp/FlightSuretyApp.spec.js\"",
    "test:contracts:app:watch": "npm run gulp -- watchContractsAndTestFilesForAppTests",
    "test:contracts:data": "truffle test ./tests/FlightSuretyData/FlightSuretyData.spec.js",
    "migrate:contracts": "node ./scripts/remove-solidity-build.js && truffle migrate --reset",
    "dapp": "npm run migrate:contracts && webpack-dev-server --mode development --config ./configs/webpack/webpack.config.dapp.js",
    "dapp:dev:sample1": "concurrently --success first --kill-others \"ganache-cli -a 10 -d -m 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat' -e 1000 -l 8000000 \"  \"npm run dapp\" \"npm run gulp -- watchContractsForDapp\"",
    "dapp:dev:sample1:windows": "cd ./scripts && keep-dapp-dev-process.bat",
    "dapp:dev:sample1:unix": "./scripts/keep-dapp-dev-process.sh",
    "dapp:prod": "webpack --mode production --config ./configs/webpack/webpack.config.dapp.js",
    "server": "rm -rf ./build/server && webpack --config ./configs/webpack/webpack.config.server.js",
    "prettier": "prettier --write 'contracts/**/*.sol'"
  },
  "author": "Jurshsmith",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bignumber.js": "8.0.2",
    "concurrently": "^6.2.0",
    "css-loader": "^1.0.0",
    "express": "4.16.4",
    "file-loader": "3.0.1",
    "ganache-cli": "^6.12.2",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "gulp-run-command": "^0.0.10",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "lite-server": "^2.5.4",
    "openzeppelin-solidity": "^1.10.0",
    "prettier": "^2.3.0",
    "prettier-plugin-solidity": "^1.0.0-beta.10",
    "start-server-webpack-plugin": "2.2.5",
    "style-loader": "^0.23.1",
    "superstatic": "6.0.3",
    "truffle": "5.3.4",
    "truffle-assertions": "^0.9.2",
    "truffle-hdwallet-provider": "1.0.2",
    "watch": "^1.0.2",
    "web3": "1.3.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "3.4.1",
    "webpack-node-externals": "1.7.2"
  }
}
